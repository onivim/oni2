    "h",
    "c",
    "h.in"
  ],
  "folds": [
    {
      "type": [
        "comment",
        "preproc_arg"
      ]
    },
    {
      "type": [
        "preproc_if",
        "preproc_ifdef",
        "preproc_elif"
      ],
      "end": {
        "type": [
          "preproc_else",
          "preproc_elif"
        ]
      }
    },
    {
      "type": [
        "preproc_if",
        "preproc_ifdef"
      ],
      "end": {
        "index": -1
      }
    },
    {
      "type": [
        "preproc_else",
        "preproc_elif"
      ],
      "start": {
        "index": 0
      }
    },
    {
      "type": [
        "enumerator_list",
        "compound_statement",
        "declaration_list",
        "field_declaration_list",
        "parameter_list",
        "argument_list",
        "initializer_list",
        "parenthesized_expression",
        "template_parameter_list",
        "template_argument_list"
      ],
      "start": {
        "index": 0
      },
      "end": {
        "index": -1
      }
    },
    {
      "type": "case_statement",
      "start": {
        "index": 0
